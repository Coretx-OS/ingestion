{
  "name": "@secondbrain/contracts",
  "version": "0.1.0",
  "description": "Shared TypeScript types and OpenAPI spec for Second Brain OS",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./openapi": "./openapi.yaml"
  },
  "files": [
    "dist",
    "openapi.yaml"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  },
  "keywords": [
    "second-brain",
    "types",
    "contracts",
    "openapi"
  ],
  "author": "Robert Cousins",
  "license": "MIT"
}
