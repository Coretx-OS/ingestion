{
  "name": "@secondbrain/sdk",
  "version": "0.1.0",
  "description": "Typed SDK for Second Brain OS API",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
